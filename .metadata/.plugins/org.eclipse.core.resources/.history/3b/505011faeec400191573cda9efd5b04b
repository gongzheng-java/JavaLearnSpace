public class Test {
	public static void main(String[] args) {
		
		try {
			Date date=new Date();
			Thread.sleep(3000);
			Date date1=new Date();
			if (date.getTime()<date1.getTime()) {
				System.out.println("11233132");
			}
			System.out.println(date.getDate());
		} catch (InterruptedException e) {
			// TODO Auto-generated catch block
			e.printStackTrace();
		}
	}

}
